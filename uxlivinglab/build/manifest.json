{
  "name": "UxLivingLab",
  "description": "livinlab",
  "version": "2.0",
  "manifest_version": 3,
  "action": {
    "default_icon": "dowlogo1.png",
    "default_popup": "index.html",
    "default_title": "Open the popup"
  },
  "content_scripts": [
    {
      "css": ["/static/css/main.bfd4bb54.css"],
      "js": ["/static/js/main.b0264cb2.js"],
      "all_frames": true,
      "run_at": "document_end",
      "matches": ["http://127.0.0.1.zxqpopw/*"]
    }
  ],

  "icons": {
    "16": "dowlogo1.png",
    "48": "dowlogo1.png",
    "128": "dowlogo1.png"
  },
  "host_permissions": ["<all_urls>"],
  "permissions": ["tabs", "bookmarks", "cookies"]
}
